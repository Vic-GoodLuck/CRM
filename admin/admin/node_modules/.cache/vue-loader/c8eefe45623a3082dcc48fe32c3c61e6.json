ss=\"login_form\"\r\n        :model=\"loginForm\"\r\n        :rules=\"loginFormRules\"\r\n        ref=\"loginFormRef\"\r\n      >\r\n        <el-form-item prop=\"username\">\r\n          <el-input\r\n            prefix-icon=\"el-icon-user\"\r\n            placeholder=\"请输入用户名\"\r\n            v-model=\"loginForm.username\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input\r\n            prefix-icon=\"iconfont icon-suoding\"\r\n            placeholder=\"请输入密码\"\r\n            :type=\"pwdType\"\r\n            v-model=\"loginForm.password\"\r\n          >\r\n          </el-input>\r\n          <div class=\"eye\" @click=\"isClosed\">\r\n            <i :class=\"eyes ? icon1 : icon2\"></i>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item prop=\"code\" style=\"width: 200px\">\r\n          <el-input\r\n            prefix-icon=\"el-icon-phone-outline\"\r\n            placeholder=\"请输入验证码\"\r\n            v-model=\"loginForm.code\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <img\r\n            style=\"width: 120px; height: 40px;margin-left:50px;margin-top:-70px !important;position:absolute;\"\r\n            :src=\"captchUrl\"\r\n            @click=\"updateCaptcha\"\r\n        />\r\n        <el-form-item class=\"form_button\">\r\n          <el-button type=\"primary\" round class=\"btn1\" @click=\"login\">登录<i class=\"el-icon-right\"></i></el-button>\r\n        </el-form-item>\r\n        <div class=\"sign_up1\" @click='jump(\"/loginEmail\")'><span>邮箱登录</span></div>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"login\",\r\n  inject: [\"reload\"],\r\n  data() {\r\n    return {\r\n      captchUrl:\"/api/captcha?time=\"+new Date(),\r\n      loginForm: {\r\n        username: \"\",\r\n        email: \"\",\r\n        number: \"\",\r\n        code: \"\",\r\n      },\r\n      pwdType: \"password\",\r\n      eyes: true,\r\n      icon1: \"iconfont icon-hide\",\r\n      icon2: \"iconfont icon-browse\",\r\n      loginFormRules: {\r\n        username: [\r\n          { required: true, message: \"请输入用户名\", trigger: \"blur\" },\r\n          // { message: '长度最多不超过 12 个字符', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n          // { max: 10, message: '长度最多不超过 10 个字符', trigger: 'blur' }\r\n        ],\r\n        email: [\r\n          { required: true, message: \"请重新输入内容\", trigger: \"blur\" },\r\n        ],\r\n        code: [\r\n          { required: true, message: \"请输入验证码\", trigger: \"blur\" },\r\n          { max: 4, message: \"长度最多不超过4个字符\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    //登录\r\n    login() {\r\n      this.$refs.loginFormRef.validate(async (valid) => {\r\n        if (!valid) return this.$message.error(\"表单验证失败！\");\r\n        console.log(this.loginForm);\r\n        var user = this.loginForm;\r\n        console.log(user);\r\n        // 登录接口\r\n        const result = await this.$http.post(\"/user/login\", this.loginForm);\r\n        console.log(\"login\", result);\r\n        if (result.data.code === 200) {\r\n          if(result.data.obj.role!==0) {\r\n            this.$message.success(\"登陆成功\");\r\n            localStorage.setItem(\"userdata\", JSON.stringify(result.data.obj));\r\n            this.$router.push(\"/page\");\r\n            console.log(\"localstorage\", JSON.parse(localStorage.getItem(\"userdata\"))\r\n            );\r\n          }else{\r\n            this.$message.error(\"您没有权限进入后台!\");\r\n          }\r\n        } else {\r\n          this.$message.error(result.data.message);\r\n          this.captchUrl=\"/api/captcha?time=\"+new Date()\r\n        }\r\n      });\r\n    },\r\n    jump(path) {\r\n       this.$router.push(path);\r\n    },\r\n    updateCaptcha(){\r\n      this.captchUrl=\"/api/captcha?time=\"+new Date()\r\n    },\r\n    isClosed() {\r\n      this.pwdType = this.pwdType === \"password\" ? \"text\" : \"password\";\r\n      this.eyes = !this.eyes;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.login_container {\r\n  height: 100%;\r\n  background-size: cover;\r\n  background: linear-gradient(to bottom right, #99cccc, #fff);\r\n}\r\n\r\n.loginForm_box {\r\n  width: 450px;\r\n  height: 350px;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.title_box {\r\n  width: 100%;\r\n  height: 85px;\r\n  line-height: 85px;\r\n  // background: green;\r\n  text-align: center;\r\n  > span {\r\n    font: 400 40px \"Times New Roman\", Georgia, Serif;\r\n    color: #88d3ce;\r\n  }\r\n}\r\n\r\n.login_form {\r\n  width: 100%;\r\n  padding: 0 60px;\r\n  box-sizing: border-box;\r\n  position: absolute;\r\n  bottom: 0;\r\n  > .el-form-item {\r\n    margin-bottom: 30px;\r\n  }\r\n}\r\n\r\n.form_button {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.sign_up {\r\n  position: absolute;\r\n  right: 17px;\r\n  bottom: 11px;\r\n  color: #909399;\r\n  cursor: pointer;\r\n}\r\n.sign_up1 {\r\n  position: absolute;\r\n  left: 17px;\r\n  bottom: 11px;\r\n  color: #909399;\r\n  cursor: pointer;\r\n}\r\n\r\n.eye {\r\n  position: absolute;\r\n  bottom: -2px;\r\n  right: 0px;\r\n}\r\n.select {\r\n  width: 330px;\r\n  margin-top: -10px;\r\n  margin-bottom: 20px;\r\n}\r\n.btn1 {\r\n  width: 100px;\r\n}\r\n.code {\r\n  width: 200px;\r\n  margin-top: -50px;\r\n}\r\n</style>\r\n"]}]}