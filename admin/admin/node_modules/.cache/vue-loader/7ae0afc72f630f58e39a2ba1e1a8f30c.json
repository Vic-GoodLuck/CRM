-->\r\n<template>\r\n    <div>\r\n        <el-card class=\"box-card\" style=\"width: 100%\">\r\n            <el-breadcrumb style=\"margin-bottom:15px;\" separator=\"/\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item :to=\"{ path: '/userdetail' }\">用户管理</el-breadcrumb-item>\r\n                <el-breadcrumb-item>用户信息</el-breadcrumb-item>\r\n            </el-breadcrumb>\r\n            <el-divider></el-divider>\r\n            <div style=\"display: flex\">\r\n                <div style=\"padding: 10px 0;\">\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入用户名\" style=\"width: 200px;margin:5px;\" v-model=\"username_select\"></el-input>\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入昵称\" style=\"width: 200px;margin:5px;\" v-model=\"nickname_select\"></el-input>\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入邮箱\" style=\"width: 200px;margin:5px;\" v-model=\"email_select\"></el-input>\r\n                    <el-button class=\"ml-5\" type=\"primary\" @click=\"conditionSelect\">搜索</el-button>\r\n                </div>\r\n                <div></div>\r\n            </div>\r\n            <!-- 添加信息 -->\r\n            <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n                <el-form-item>\r\n                    <!--          权限分配弹出框-->\r\n                    <el-dialog\r\n                            title=\"权限分配\"\r\n                            :visible.sync=\"dialogFormVisibleRole\"\r\n                            width=\"30%\"\r\n                            append-to-body\r\n                            center\r\n                    >\r\n                        <el-tree\r\n                                :data=\"RoleData\"\r\n                                show-checkbox\r\n                                node-key=\"id\"\r\n                                ref=\"tree\"\r\n                                :default-expanded-keys=\"[1, 2, 3,4,5,6,17]\"\r\n                                :default-checked-keys=\"checks\"\r\n                                style=\"margin-left: 10%;\">\r\n                            <span class=\"custom-tree-node\" slot-scope=\"{node,data}\">\r\n                               <span><i :class=\"data.icon\"></i> {{ node.label }}</span>\r\n                            </span>\r\n                        </el-tree>\r\n\r\n                        <div slot=\"footer\" class=\"dialog-footer\">\r\n                            <el-button type=\"primary\" @click=\"updateRole()\">确定</el-button>\r\n                            <el-button @click=\"dialogFormVisibleRole = false\">取消</el-button>\r\n                        </div>\r\n                    </el-dialog>\r\n\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!--      用户信息表格-->\r\n            <el-table\r\n                    :data=\"tableData\"\r\n                    border\r\n                    :row-style=\"{ height: '15px' }\"\r\n                    :cell-style=\"{ padding: '0px' }\"\r\n                    max-height=\"1000\"\r\n                    style=\"width: 67%\"\r\n                    @selection-change=\"handleSelectionChange\">\r\n                >\r\n\r\n                <el-table-column fixed label=\"用户头像\" width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-image v-if=\"scope.row.avatar !== ''\"\r\n                                  style=\"width: 80px;\"\r\n                                  :src=\"scope.row.avatar\"\r\n                                  fit=\"contain\"\r\n                        ></el-image>\r\n                        <span v-else>无图片</span>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"username\" label=\"用户名\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"email\" label=\"邮箱\" width=\"160\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"roleName\" label=\"身份\" width=\"130\">\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" width=\"150\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button @click=\"updateClick(scope.row)\" type=\"primary\" size=\"small\">权限管理 <i class=\"el-icon-s-operation\"></i></el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <div class=\"block\" style=\"margin-top: 20px;\">\r\n                <el-pagination\r\n                        @size-change=\"handleSizeChange\"\r\n                        @current-change=\"handleCurrentChange\"\r\n                        :current-page.sync=\"currentPage\"\r\n                        :page-size=\"pagesize\"\r\n                        layout=\"prev, pager, next, jumper,->,total\"\r\n                        :total=\"total\"\r\n                >\r\n                </el-pagination>\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        mounted() {\r\n            this.$http.get(\r\n                `/role/getRole?uid=${JSON.parse(localStorage.getItem('userdata')).id}`,{\r\n                    headers: {\r\n                        Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                    }\r\n                }\r\n            ).then(res=>{\r\n                let RoleName=res.data.obj;\r\n                if(RoleName.includes(this.pageName)){\r\n                    this.hasRole=true;\r\n                }else{\r\n                    this.hasRole=false;\r\n                }\r\n                if(this.hasRole==false){\r\n                    this.$message.error(\"无权访问\");\r\n                    this.jump(\"/404\");\r\n                    return;\r\n                }\r\n                this.userdata();\r\n                this.$forceUpdate();\r\n            })\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                hasRole:null,\r\n                pageName:\"权限管理\",\r\n\r\n\r\n                options: [\r\n                    {\r\n                        value: \"普通用户\",\r\n                        label: \"普通用户\",\r\n                    },\r\n                    {\r\n                        value: \"管理员\",\r\n                        label: \"管理员\",\r\n                    },\r\n                ],\r\n                RoleData:[\r\n                    {\r\n                        id: 1,\r\n                        label: '首页',\r\n                        icon:\"el-icon-s-home\",\r\n                    },\r\n                    {\r\n                        id: 17,\r\n                        label: '轮播视频',\r\n                        icon:\"el-icon-video-camera\",\r\n                        children: [{\r\n                            id: 18,\r\n                            label: '轮播列表',\r\n                            icon:\"el-icon-video-play\",\r\n                        },{\r\n                            id: 19,\r\n                            label: '视频上传',\r\n                            icon:\"el-icon-upload2\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 2,\r\n                        label: '文章管理',\r\n                        icon:\"el-icon-notebook-2\",\r\n                        children: [{\r\n                            id: 7,\r\n                            label: '文章列表',\r\n                            icon:\"el-icon-tickets\",\r\n                        },{\r\n                            id: 8,\r\n                            label: '文章审核',\r\n                            icon:\"el-icon-document-checked\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 3,\r\n                        label: '视频管理',\r\n                        icon:\"el-icon-film\",\r\n                        children: [{\r\n                            id: 9,\r\n                            label: '视频列表',\r\n                            icon:\"el-icon-video-camera\",\r\n                        },{\r\n                            id: 10,\r\n                            label: '视频审核',\r\n                            icon:\"el-icon-document-checked\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 4,\r\n                        label: '用户管理',\r\n                        icon:\"el-icon-user\",\r\n                        children: [{\r\n                            id: 11,\r\n                            label: '用户信息',\r\n                            icon:\"el-icon-user-solid\",\r\n                        }\r\n                        // ,{\r\n                        //     id: 12,\r\n                        //     label: '权限管理',\r\n                        //     icon:\"el-icon-s-custom\",\r\n                        // }\r\n                        ]\r\n                    },\r\n                    {\r\n                        id: 5,\r\n                        label: '题库管理',\r\n                        icon:\"el-icon-collection\",\r\n                        children: [{\r\n                            id: 13,\r\n                            label: '题库列表',\r\n                            icon:\"el-icon-receiving\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 6,\r\n                        label: '商店管理',\r\n                        icon:\"el-icon-s-shop\",\r\n                        children: [{\r\n                            id: 14,\r\n                            label: '商品信息',\r\n                            icon:\"el-icon-goods\",\r\n                        },{\r\n                            id: 15,\r\n                            label: '订单管理',\r\n                            icon:\"el-icon-shopping-cart-full\",\r\n                        },{\r\n                            id: 16,\r\n                            label: '退款审核',\r\n                            icon:\"el-icon-s-check\",\r\n                        }]\r\n                    },\r\n                ],\r\n\r\n                checks:[],\r\n\r\n                target_uid:0,\r\n\r\n                dialogFormVisibleRole:false,\r\n\r\n                username_select:\"\",\r\n                nickname_select:\"\",\r\n                email_select:\"\",\r\n\r\n                multipleSelection:[],\r\n                total: 1000,\r\n                // 分页\r\n                //currentPage: 1, //初始页\r\n                pagesize: 5, //每页多少数据\r\n                currentPage: 1, //默认当前页为第一页\r\n\r\n                formLabelWidth: \"100px\",\r\n                formInline: {\r\n                    user: \"\",\r\n                },\r\n                tableData: [],\r\n\r\n            };\r\n        },\r\n        methods: {\r\n            //this.jump(\"/404\");\r\n            jump(path) {\r\n                this.$router.push(path);\r\n            },\r\n            // 获取第一页表格数据\r\n            async userdata() {\r\n                const result = await this.$http.get(\r\n                    `/user/adminlist?currentPage=${1}&size=${5}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.tableData = result.data.data;\r\n                    this.total = Number(result.data.total);\r\n                    this.$forceUpdate();\r\n                }\r\n            },\r\n            handleSelectionChange(val){\r\n                this.multipleSelection=val;\r\n                console.log(val);\r\n            },\r\n\r\n            async updateClick(row) {\r\n                // 点击获取信息\r\n                this.dialogFormVisibleRole = true;\r\n                this.target_uid = row.id;\r\n                const result = await this.$http.get(\r\n                    `/role/getRole?uid=${this.target_uid}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                this.$refs.tree.setCheckedKeys([]);\r\n\r\n                let m = new Map();\r\n                m.set(\"首页\",1);\r\n                m.set(\"文章管理\",2);\r\n                m.set(\"文章列表\",7);\r\n                m.set(\"文章审核\",8);\r\n                m.set(\"视频管理\",3);\r\n                m.set(\"视频列表\",9);\r\n                m.set(\"视频审核\",10);\r\n                m.set(\"用户管理\",4);\r\n                m.set(\"用户信息\",11);\r\n                m.set(\"权限管理\",12);\r\n                m.set(\"题库管理\",5);\r\n                m.set(\"题库列表\",13);\r\n                m.set(\"商店管理\",6);\r\n                m.set(\"商品信息\",14);\r\n                m.set(\"订单管理\",15);\r\n                m.set(\"退款审核\",16);\r\n                if (result.status === 200) {\r\n                    let RoleName = result.data.obj;\r\n                    this.checks = [];\r\n                    for (let i = 0; i < RoleName.length; i++) {\r\n                        this.checks.push(m.get(RoleName[i]));\r\n                    }\r\n                    console.log(this.checks);\r\n                    this.$message.success(\"成功获得权限信息\");\r\n                }else{\r\n                    this.$message.error(\"获得权限信息失败\");\r\n                }\r\n\r\n            },\r\n\r\n            async updateRole(){\r\n                let t=this.$refs.tree.getCheckedNodes();\r\n                let rolename=[];\r\n                for (let i = 0; i < t.length ; i++) {\r\n                    rolename.push(t[i].label);\r\n                }\r\n                console.log(rolename);\r\n                const result = await this.$http.get(\r\n                    `/role/update?uid=${this.target_uid}&role=${rolename}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.$message.success(\"权限分配成功\");\r\n                }else{\r\n                    this.$message.error(\"权限分配失败\");\r\n                }\r\n                this.dialogFormVisibleRole = false;\r\n            },\r\n\r\n\r\n            handleSizeChange(val) {\r\n                console.log(`每页 ${val} 条`);\r\n            },\r\n\r\n            async handleCurrentChange(val) {\r\n                console.log(`当前页: ${val}`);\r\n                if(this.username_select==\"\"&&this.nickname_select==\"\"&&this.email_select==\"\"){\r\n                    const result = await this.$http.get(\r\n                        `/user/adminlist?currentPage=${val}&size=${5}`,{\r\n                            headers: {\r\n                                Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                            }\r\n                        }\r\n                    );\r\n                    if (result.status === 200) {\r\n                        this.tableData = result.data.data;\r\n                        this.total = Number(result.data.total);\r\n                        this.$forceUpdate();\r\n                    }\r\n                }else{\r\n                    const result = await this.$http.get(\r\n                        `/user/conditionAdminSelect?currentPage=${val}&size=${5}&username=${this.username_select}&nickname=${this.nickname_select}&email=${this.email_select}`,{\r\n                            headers: {\r\n                                Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                            }\r\n                        }\r\n                    );\r\n                    if (result.status === 200) {\r\n                        this.tableData = result.data.data;\r\n                        this.total = Number(result.data.total);\r\n                        this.$forceUpdate();\r\n                    } else {\r\n                        this.$message.error(\"查询失败\");\r\n                    }\r\n                }\r\n\r\n            },\r\n\r\n            //条件查询\r\n            async conditionSelect(){\r\n                this.currentPage=1;\r\n                const result = await this.$http.get(\r\n                    `/user/conditionAdminSelect?currentPage=${1}&size=${5}&username=${this.username_select}&nickname=${this.nickname_select}&email=${this.email_select}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.tableData = result.data.data;\r\n                    this.total = Number(result.data.total);\r\n                    this.$forceUpdate();\r\n                    this.$message.success(\"查询成功\");\r\n                } else {\r\n                    this.$message.error(\"查询失败\");\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .carid {\r\n        display: none;\r\n    }\r\n    .demo-form-inline {\r\n        display: flex;\r\n        flex: left;\r\n    }\r\n    .el-form-item {\r\n        margin-bottom: 20px;\r\n    }\r\n    .avatar-uploader .el-upload {\r\n        border: 1px dashed #d9d9d9;\r\n        border-radius: 100px;\r\n        cursor: pointer;\r\n        position: relative;\r\n        overflow: hidden;\r\n    }\r\n    // .avatar-uploader .el-upload:hover {\r\n    // //   border-color: #409eff;\r\n    // }\r\n    .avatar-uploader-icon {\r\n        font-size: 28px;\r\n        color: #8c939d;\r\n        width: 178px;\r\n        height: 178px;\r\n        line-height: 178px;\r\n        text-align: center;\r\n    }\r\n    .avatar {\r\n        width: 178px;\r\n        height: 178px;\r\n        display: block;\r\n        border-radius: 100px;\r\n    }\r\n    .custom-tree-node {\r\n        flex: 1;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        font-size: 14px;\r\n        padding-right: 8px;\r\n    }\r\n\r\n    .upload .el-upload__input {\r\n        display: none;\r\n    }\r\n\r\n    input[type=\"file\"] {\r\n        display: none !important;\r\n    }\r\n</style>\r\n"]}]}