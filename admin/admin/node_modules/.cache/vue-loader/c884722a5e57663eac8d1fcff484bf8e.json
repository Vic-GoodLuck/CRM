{"remainingRequest":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\src\\views\\roledetail\\index.vue?vue&type=style&index=0&id=39dd67b8&lang=less&scoped=true&","dependencies":[{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\src\\views\\roledetail\\index.vue","mtime":1670338810309},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\css-loader\\dist\\cjs.js","mtime":1666323413935},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1666323416977},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\postcss-loader\\src\\index.js","mtime":1666323415315},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\less-loader\\dist\\cjs.js","mtime":1666323414827},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1666323412932},{"path":"C:\\Users\\Lenovo\\Desktop\\blog\\blogadmin\\node_modules\\vue-loader\\lib\\index.js","mtime":1666323415965}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAywBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/roledetail","sourcesContent":["<!--用户列表-->\r\n<template>\r\n    <div>\r\n        <el-card class=\"box-card\" style=\"width: 100%\">\r\n            <el-breadcrumb style=\"margin-bottom:15px;\" separator=\"/\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item :to=\"{ path: '/userdetail' }\">用户管理</el-breadcrumb-item>\r\n                <el-breadcrumb-item>用户信息</el-breadcrumb-item>\r\n            </el-breadcrumb>\r\n            <el-divider></el-divider>\r\n            <div style=\"display: flex\">\r\n                <div style=\"padding: 10px 0;\">\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入用户名\" style=\"width: 200px;margin:5px;\" v-model=\"username_select\"></el-input>\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入昵称\" style=\"width: 200px;margin:5px;\" v-model=\"nickname_select\"></el-input>\r\n                    <el-input class=\"ml-5\" suffix-icon=\"el-icon-search\" placeholder=\"请输入手机号\" style=\"width: 200px;margin:5px;\" v-model=\"phone_select\"></el-input>\r\n                    <el-button class=\"ml-5\" type=\"primary\" @click=\"conditionSelect\">搜索</el-button>\r\n                </div>\r\n                <div></div>\r\n            </div>\r\n            <!-- 添加信息 -->\r\n            <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n                <el-form-item>\r\n                    <div>\r\n                        <el-button type=\"primary\" @click=\"dialogFormVisible = true\">添加 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n                        <el-button type=\"danger\" @click=\"deleteBatch\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n                    </div>\r\n                    <!--          添加用户弹出框-->\r\n                    <el-dialog\r\n                            title=\"添加用户\"\r\n                            :visible.sync=\"dialogFormVisible\"\r\n                            width=\"40%\"\r\n                            append-to-body\r\n                            center\r\n                    >\r\n                        <el-upload\r\n                                class=\"avatar-uploader\"\r\n                                :action=\"doupload()\"\r\n                                :show-file-list=\"false\"\r\n                                :on-success=\"handleAvatarSuccess\"\r\n                                :before-upload=\"beforeAvatarUpload\"\r\n                                style=\"display: inline-block; width: 120px; margin-left: 35%;margin-bottom: 20px;\">\r\n                            <!-- 上传成功后展示的图片 -->\r\n                            <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\" />\r\n                            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                        </el-upload>\r\n                        <el-form\r\n                                :model=\"form\"\r\n                                style=\":form\"\r\n                                :rules=\"loginFormRules\"\r\n                                ref=\"loginFormRef\"\r\n                        >\r\n                            <el-form-item\r\n                                    label=\"用户名:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"username\"\r\n                            >\r\n                                <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"昵称:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"username\"\r\n                            >\r\n                                <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"手机号码:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"phone\"\r\n                            >\r\n                                <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"密码:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"password\"\r\n                            >\r\n                                <el-input v-model=\"form.password\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"身份:\" :label-width=\"formLabelWidth\">\r\n                                <el-select v-model=\"form.role\" placeholder=\"请选择身份\">\r\n                                    <el-option label=\"管理员\" value=\"1\"></el-option>\r\n                                    <el-option label=\"用户\" value=\"0\"></el-option>\r\n                                </el-select>\r\n                            </el-form-item>\r\n                        </el-form>\r\n                        <div slot=\"footer\" class=\"dialog-footer\">\r\n                            <el-button type=\"primary\" @click=\"submitadd()\">添加</el-button>\r\n                            <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n                        </div>\r\n                    </el-dialog>\r\n\r\n                    <!--          权限分配弹出框-->\r\n                    <el-dialog\r\n                            title=\"权限分配\"\r\n                            :visible.sync=\"dialogFormVisibleRole\"\r\n                            width=\"30%\"\r\n                            append-to-body\r\n                            center\r\n                    >\r\n                        <el-tree\r\n                                :data=\"RoleData\"\r\n                                show-checkbox\r\n                                node-key=\"id\"\r\n                                ref=\"tree\"\r\n                                :default-expanded-keys=\"[1, 2, 3,4,5,6]\"\r\n                                :default-checked-keys=\"checks\"\r\n                                style=\"margin-left: 10%;\">\r\n                            <span class=\"custom-tree-node\" slot-scope=\"{node,data}\">\r\n                               <span><i :class=\"data.icon\"></i> {{ node.label }}</span>\r\n                            </span>\r\n                        </el-tree>\r\n\r\n                        <div slot=\"footer\" class=\"dialog-footer\">\r\n                            <el-button type=\"primary\" @click=\"updateRole()\">确定</el-button>\r\n                            <el-button @click=\"dialogFormVisibleRole = false\">取消</el-button>\r\n                        </div>\r\n                    </el-dialog>\r\n\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!--      用户信息表格-->\r\n            <el-table\r\n                    :data=\"tableData\"\r\n                    border\r\n                    :row-style=\"{ height: '15px' }\"\r\n                    :cell-style=\"{ padding: '0px' }\"\r\n                    max-height=\"1000\"\r\n                    style=\"width: 85%\"\r\n                    @selection-change=\"handleSelectionChange\">\r\n                >\r\n                <el-table-column\r\n                        type=\"selection\"\r\n                        width=\"50\">\r\n                </el-table-column>\r\n                <!--        <el-table-column fixed v-if=\"false\" prop=\"id\" label=\"用户id\" width=\"100\"></el-table-column>-->\r\n                <el-table-column fixed label=\"用户头像\" width=\"100\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-image v-if=\"scope.row.avatar !== ''\"\r\n                                  style=\"width: 80px;\"\r\n                                  :src=\"scope.row.avatar\"\r\n                                  fit=\"contain\"\r\n                        ></el-image>\r\n                        <span v-else>无图片</span>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"username\" label=\"用户名\" width=\"140\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"nickname\" label=\"昵称\" width=\"140\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"phone\" label=\"电话\" width=\"160\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"roleName\" label=\"身份\" width=\"140\">\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" width=\"280\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button @click=\"updateClick(scope.row)\" type=\"primary\" size=\"small\">权限管理 <i class=\"el-icon-s-operation\"></i></el-button>\r\n                        <el-button @click=\"handleClick(scope.row)\" type=\"success\" size=\"small\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n                        <el-button @click=\"deteleUser(scope.row)\" type=\"danger\" size=\"small\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n                        <!-- 编辑用户信息 -->\r\n                        <el-dialog\r\n                                title=\"修改用户信息\"\r\n                                :visible.sync=\"dialogFormVisible1\"\r\n                                width=\"40%\"\r\n                                append-to-body\r\n                                center\r\n                        >\r\n                            <el-upload\r\n                                    class=\"avatar-uploader\"\r\n                                    :action=\"doupload()\"\r\n                                    :show-file-list=\"false\"\r\n                                    :on-success=\"handleAvatarSuccess1\"\r\n                                    :before-upload=\"beforeAvatarUpload1\"\r\n                                    style=\"display: inline-block; width: 120px; margin-left: 35%;margin-bottom: 20px;\">\r\n                                <!-- 上传成功后展示的图片 -->\r\n                                <img v-if=\"form1.avatar\" :src=\"form1.avatar\" class=\"avatar\" />\r\n                                <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                            </el-upload>\r\n                            <el-form\r\n                                    :model=\"form1\"\r\n                                    style=\":form1\"\r\n                                    :rules=\"loginFormRules\"\r\n                                    ref=\"loginFormRef\"\r\n                            >\r\n                                <el-form-item\r\n                                        label=\"用户名:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"username\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.username\"\r\n                                            autocomplete=\"off\"\r\n                                            disabled\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"昵称:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"username\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.nickname\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"手机号码:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"phone\"\r\n                                >\r\n                                    <el-input v-model=\"form1.phone\" autocomplete=\"off\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"密码:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"password\"\r\n                                        style=\"display: none\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.password\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"身份:\" :label-width=\"formLabelWidth\">\r\n                                    <el-select\r\n                                            v-model=\"form1.roleName\"\r\n                                            :placeholder=\"form1.roleName\"\r\n                                    >\r\n                                        <el-option\r\n                                                v-for=\"item in options\"\r\n                                                :key=\"item.value\"\r\n                                                :label=\"item.label\"\r\n                                                :value=\"item.value\"\r\n                                        >\r\n                                        </el-option>\r\n                                    </el-select>\r\n                                </el-form-item>\r\n                            </el-form>\r\n                            <div slot=\"footer\" class=\"dialog-footer\">\r\n                                <el-button type=\"primary\" @click=\"editdetail()\">提交</el-button>\r\n                                <el-button @click=\"dialogFormVisible1 = false\">取消</el-button>\r\n                            </div>\r\n                        </el-dialog>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <div class=\"block\" style=\"margin-top: 20px;\">\r\n                <el-pagination\r\n                        background\r\n                        @size-change=\"handleSizeChange\"\r\n                        @current-change=\"handleCurrentChange\"\r\n                        :current-page.sync=\"currentPage\"\r\n                        :page-size=\"pagesize\"\r\n                        layout=\"prev, pager, next, jumper\"\r\n                        :total=\"total\"\r\n                >\r\n                </el-pagination>\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        mounted() {\r\n            this.$http.get(\r\n                `/role/getRole?uid=${JSON.parse(localStorage.getItem('userdata')).id}`,{\r\n                    headers: {\r\n                        Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                    }\r\n                }\r\n            ).then(res=>{\r\n                let RoleName=res.data.obj;\r\n                if(RoleName.includes(this.pageName)){\r\n                    this.hasRole=true;\r\n                }else{\r\n                    this.hasRole=false;\r\n                }\r\n                if(this.hasRole==false){\r\n                    this.$message.error(\"无权访问\");\r\n                    return;\r\n                }\r\n                this.userdata();\r\n            })\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                hasRole:null,\r\n                pageName:\"权限管理\",\r\n\r\n\r\n                options: [\r\n                    {\r\n                        value: \"普通用户\",\r\n                        label: \"普通用户\",\r\n                    },\r\n                    {\r\n                        value: \"管理员\",\r\n                        label: \"管理员\",\r\n                    },\r\n                ],\r\n                RoleData:[\r\n                    {\r\n                        id: 1,\r\n                        label: '首页',\r\n                        icon:\"el-icon-s-home\",\r\n                    },\r\n                    {\r\n                        id: 2,\r\n                        label: '文章管理',\r\n                        icon:\"el-icon-notebook-2\",\r\n                        children: [{\r\n                            id: 7,\r\n                            label: '文章列表',\r\n                            icon:\"el-icon-tickets\",\r\n                        },{\r\n                            id: 8,\r\n                            label: '文章审核',\r\n                            icon:\"el-icon-document-checked\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 3,\r\n                        label: '视频管理',\r\n                        icon:\"el-icon-film\",\r\n                        children: [{\r\n                            id: 9,\r\n                            label: '视频列表',\r\n                            icon:\"el-icon-video-camera\",\r\n                        },{\r\n                            id: 10,\r\n                            label: '视频审核',\r\n                            icon:\"el-icon-document-checked\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 4,\r\n                        label: '用户管理',\r\n                        icon:\"el-icon-user\",\r\n                        children: [{\r\n                            id: 11,\r\n                            label: '用户信息',\r\n                            icon:\"el-icon-user-solid\",\r\n                        },{\r\n                            id: 12,\r\n                            label: '权限管理',\r\n                            icon:\"el-icon-s-custom\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 5,\r\n                        label: '题库管理',\r\n                        icon:\"el-icon-collection\",\r\n                        children: [{\r\n                            id: 13,\r\n                            label: '题库列表',\r\n                            icon:\"el-icon-receiving\",\r\n                        }]\r\n                    },\r\n                    {\r\n                        id: 6,\r\n                        label: '商店管理',\r\n                        icon:\"el-icon-s-shop\",\r\n                        children: [{\r\n                            id: 14,\r\n                            label: '商品信息',\r\n                            icon:\"el-icon-goods\",\r\n                        },{\r\n                            id: 15,\r\n                            label: '订单管理',\r\n                            icon:\"el-icon-shopping-cart-full\",\r\n                        },{\r\n                            id: 16,\r\n                            label: '退款审核',\r\n                            icon:\"el-icon-s-check\",\r\n                        }]\r\n                    },\r\n                ],\r\n\r\n                checks:[],\r\n\r\n                target_uid:0,\r\n\r\n                dialogFormVisibleRole:false,\r\n\r\n                username_select:\"\",\r\n                nickname_select:\"\",\r\n                phone_select:\"\",\r\n\r\n                multipleSelection:[],\r\n                total: 1000,\r\n                // 分页\r\n                //currentPage: 1, //初始页\r\n                pagesize: 5, //每页多少数据\r\n                currentPage: 1, //默认当前页为第一页\r\n                //添加用户对话框\r\n                dialogFormVisible: false,\r\n                form: {\r\n                    avatar:\"https://cube.elemecdn.com/6/94/4d3ea53c084bad6931a56d5158a48jpeg.jpeg\",\r\n                    username: \"\",\r\n                    nickname: \"\",\r\n                    password: \"\",\r\n                    role: \"\",\r\n                    phone: \"\",\r\n                },\r\n                //修改用户的对话框\r\n                dialogFormVisible1: false,\r\n                form1: {\r\n                    username: \"\",\r\n                    nickname: \"\",\r\n                    password: \"\",\r\n                    role: \"\",\r\n                    phone: \"\",\r\n                    avatar: \"\",\r\n                },\r\n                formLabelWidth: \"100px\",\r\n                formInline: {\r\n                    user: \"\",\r\n                },\r\n                tableData: [],\r\n                loginFormRules: {\r\n                    username: [\r\n                        { required: true, message: \"请输入用户名\", trigger: \"blur\" },\r\n                        // { message: '长度最多不超过 12 个字符', trigger: 'blur' }\r\n                    ],\r\n                    password: [\r\n                        { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n                        // { max: 10, message: '长度最多不超过 10 个字符', trigger: 'blur' }\r\n                    ],\r\n                    phone: [\r\n                        { required: true, message: \"请重新输入手机号码\", trigger: \"blur\" },\r\n                        {\r\n                            pattern:\r\n                                /^((13[0-9])|(14[0-9])|(15[0-9])|(17[0-9])|(18[0-9])|(19[0-9]))\\d{8}$/,\r\n                            message: \"请输入有效的手机号码\",\r\n                        },\r\n                        // { max: 10, message: '长度最多不超过 10 个字符', trigger: 'blur' }\r\n                    ],\r\n                },\r\n            };\r\n        },\r\n        methods: {\r\n\r\n            // 获取第一页表格数据\r\n            async userdata() {\r\n                const result = await this.$http.get(\r\n                    `/user/adminlist?currentPage=${1}&size=${5}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.tableData = result.data.data;\r\n                    this.total = Number(result.data.total);\r\n                }\r\n            },\r\n            handleSelectionChange(val){\r\n                this.multipleSelection=val;\r\n                console.log(val);\r\n            },\r\n\r\n            async updateClick(row) {\r\n                // 点击获取信息\r\n                this.dialogFormVisibleRole = true;\r\n                this.target_uid = row.id;\r\n                const result = await this.$http.get(\r\n                    `/role/getRole?uid=${this.target_uid}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                this.$refs.tree.setCheckedKeys([]);\r\n\r\n                let m = new Map();\r\n                m.set(\"首页\",1);\r\n                m.set(\"文章管理\",2);\r\n                m.set(\"文章列表\",7);\r\n                m.set(\"文章审核\",8);\r\n                m.set(\"视频管理\",3);\r\n                m.set(\"视频列表\",9);\r\n                m.set(\"视频审核\",10);\r\n                m.set(\"用户管理\",4);\r\n                m.set(\"用户信息\",11);\r\n                m.set(\"权限管理\",12);\r\n                m.set(\"题库管理\",5);\r\n                m.set(\"题库列表\",13);\r\n                m.set(\"商店管理\",6);\r\n                m.set(\"商品信息\",14);\r\n                m.set(\"订单管理\",15);\r\n                m.set(\"退款审核\",16);\r\n                if (result.status === 200) {\r\n                    let RoleName = result.data.obj;\r\n                    this.checks = [];\r\n                    for (let i = 0; i < RoleName.length; i++) {\r\n                        this.checks.push(m.get(RoleName[i]));\r\n                    }\r\n                    console.log(this.checks);\r\n                    this.$message.success(\"成功获得权限信息\");\r\n                }else{\r\n                    this.$message.error(\"获得权限信息失败\");\r\n                }\r\n\r\n            },\r\n\r\n            async updateRole(){\r\n                let t=this.$refs.tree.getCheckedNodes();\r\n                let rolename=[];\r\n                for (let i = 0; i < t.length ; i++) {\r\n                    rolename.push(t[i].label);\r\n                }\r\n                console.log(rolename);\r\n                const result = await this.$http.get(\r\n                    `/role/update?uid=${this.target_uid}&role=${rolename}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.$message.success(\"权限分配成功\");\r\n                }else{\r\n                    this.$message.error(\"权限分配失败\");\r\n                }\r\n                this.dialogFormVisibleRole = false;\r\n            },\r\n\r\n            handleClick(row) {\r\n                // 点击编辑获取信息\r\n                this.dialogFormVisible1 = true;\r\n                var tmp=Object.assign({},row);\r\n                this.form1 = tmp;\r\n                console.log(row);\r\n            },\r\n            handleSizeChange(val) {\r\n                console.log(`每页 ${val} 条`);\r\n            },\r\n\r\n            async handleCurrentChange(val) {\r\n                console.log(`当前页: ${val}`);\r\n                if(this.username_select==\"\"&&this.nickname_select==\"\"&&this.phone_select==\"\"){\r\n                    const result = await this.$http.get(\r\n                        `/user/adminlist?currentPage=${val}&size=${5}`,{\r\n                            headers: {\r\n                                Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                            }\r\n                        }\r\n                    );\r\n                    if (result.status === 200) {\r\n                        this.tableData = result.data.data;\r\n                        this.total = Number(result.data.total);\r\n                    }\r\n                }else{\r\n                    const result = await this.$http.get(\r\n                        `/user/conditionAdminSelect?currentPage=${val}&size=${5}&username=${this.username_select}&nickname=${this.nickname_select}&phone=${this.phone_select}`,{\r\n                            headers: {\r\n                                Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                            }\r\n                        }\r\n                    );\r\n                    if (result.status === 200) {\r\n                        this.tableData = result.data.data;\r\n                        this.total = Number(result.data.total);\r\n                        this.$forceUpdate();\r\n                    } else {\r\n                        thi