 this.$message.success(res.message);\r\n                this.handleSizeChange();\r\n              } else {\r\n                this.$message.error(res.message);\r\n              }\r\n            } else {\r\n              this.$message.error(resp.data.message);\r\n            }\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    initArticle(){\r\n      // eslint-disable-next-line no-unused-vars\r\n      this.$http.get(\"/article/list?currentPage=\"+this.currentPage+\"&size=\"+this.size).then(resp=>{\r\n        // eslint-disable-next-line no-empty\r\n        if(resp.status===200){\r\n          this.total = resp.data.total;\r\n          this.article = resp.data.data;\r\n        }\r\n      })\r\n    },\r\n /*   // 初始页currentPage、初始每页数据数pagesize和数据data\r\n    handleSizeChange: async function (size) {\r\n      var id = JSON.parse(localStorage.getItem(\"userdata\")).id;\r\n      const result = await this.$http.get(\r\n        `/article/list?currentPage=${this.currentPage}&size=${10}`\r\n      );\r\n      if (result.status == 200) {\r\n        var that = this;\r\n        var pagelist = result.data;\r\n        that.article = pagelist.data;\r\n        that.pagelength = result.data.total;\r\n        console.log(that.article);\r\n      } else {\r\n        this.$message.error(\"获取文章数据失败！\");\r\n      }\r\n    },\r\n    handleCurrentChange: async function (currentPage) {\r\n      this.currentPage = currentPage;\r\n      console.log(this.currentPage); //点击第几页\r\n      var id = JSON.parse(localStorage.getItem(\"userdata\")).id;\r\n      const result = await this.$http.get(\r\n        `/article/list?currentPage=${this.currentPage}&size=${10}`\r\n      );\r\n      // console.log(result.data);\r\n      if (result.status == 200) {\r\n        var that = this;\r\n        var pagelist = result.data;\r\n        that.article = pagelist.data;\r\n