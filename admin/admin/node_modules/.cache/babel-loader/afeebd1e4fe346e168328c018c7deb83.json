n        children:[\r\n            {\r\n                path:'/',\r\n                component:()=>import('@/views/home.vue')\r\n            },\r\n            {\r\n                path:'/home',\r\n                component:()=>import('@/views/home.vue')\r\n            },\r\n            {\r\n                path:'/articlelist',\r\n                component:()=>import('@/views/articlelist')\r\n            },\r\n            {\r\n                path:'/articlecheck',\r\n                component:()=>import('@/views/articlelist/articlecheck')\r\n            },\r\n            // 文章详情页面\r\n            {\r\n                path:'/articledetail',\r\n                component:()=>import('@/components/articledetail')\r\n            },\r\n            {\r\n                path:'/userdetail',\r\n                component:()=>import('@/views/userdetail')\r\n            },\r\n            {\r\n                path:'/questiondetail',\r\n                component:()=>import('@/views/questiondetail')\r\n            },\r\n            {\r\n                path:'/commoditydetail',\r\n                component:()=>import('@/views/commoditydetail')\r\n            },\r\n            // 视频播放管理\r\n            {\r\n                path:'/video',\r\n                component:()=>import('@/views/play')\r\n            },\r\n            {\r\n                path:'/videodetail',\r\n                component:()=>import('@/views/play/play.vue')\r\n            },\r\n            {\r\n                path:'/videocheck',\r\n                component:()=>import('@/views/play/videocheck')\r\n            },\r\n            // 管理员页面\r\n           \r\n            {\r\n                path:'/edituser',\r\n                // 编辑个人资料\r\n                component:()=>import('@/components/editdetail.vue')\r\n            },\r\n            {\r\n                path:'/changepwd',\r\n                // 修改密码\r\n                component:()=>import('@/components/changepassword.vue')\r\n            },\r\n        ]\r\n    }\r\n]\r\nconst routers = new VueRouter({routes})\r\nrouters.beforeEach(async (to, from, next) => {\r\n    // console.log('to',to)\r\n    // console.log('from',from)\r\n    // console.log('next',next)\r\n    var userinfo = JSON.parse(localStorage.getItem('userdata'))\r\n    console.log(userinfo)\r\n\r\n    if(userinfo){\r\n        if (to.path === '/login') {\r\n            next('/home')\r\n        } else {\r\n            next()\r\n        }\r\n    }else{//未登录只能去登陆注册\r\n        if (to.path === '/login') {\r\n            next()\r\n        } else {\r\n            next('/login')\r\n        }\r\n    }\r\n})\r\nexport default routers"]}]}