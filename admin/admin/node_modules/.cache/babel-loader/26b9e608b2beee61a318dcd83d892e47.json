                center\r\n                    >\r\n                        <el-upload\r\n                                class=\"avatar-uploader\"\r\n                                :action=\"doupload()\"\r\n                                :show-file-list=\"false\"\r\n                                :on-success=\"handleAvatarSuccess\"\r\n                                :before-upload=\"beforeAvatarUpload\"\r\n                                style=\"display: inline-block; width: 120px; margin-left: 30%;margin-bottom: 20px;\">\r\n                            <!-- 上传成功后展示的图片 -->\r\n                            <img v-if=\"form.img\" :src=\"form.img\" class=\"avatar\" />\r\n                            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                        </el-upload>\r\n                        <el-form\r\n                                :model=\"form\"\r\n                                style=\":form\"\r\n                                :rules=\"loginFormRules\"\r\n                                ref=\"loginFormRef\"\r\n                        >\r\n                            <el-form-item\r\n                                    label=\"商品名称:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"name\"\r\n                            >\r\n                                <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"商品价格:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"price\"\r\n                            >\r\n                                <el-input v-model=\"form.price\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"所需积分:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"point\"\r\n                            >\r\n                                <el-input v-model=\"form.point\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item\r\n                                    label=\"库存数量:\"\r\n                                    :label-width=\"formLabelWidth\"\r\n                                    prop=\"amount\"\r\n                            >\r\n                                <el-input v-model=\"form.amount\" autocomplete=\"off\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"商品类型:\" :label-width=\"formLabelWidth\">\r\n                                <el-select v-model=\"form.kind\" placeholder=\"请选择类型\">\r\n                                    <el-option label=\"虚拟商品\" value=\"1\"></el-option>\r\n                                    <el-option label=\"普通商品\" value=\"2\"></el-option>\r\n                                    <el-option label=\"高阶商品\" value=\"3\"></el-option>\r\n                                </el-select>\r\n                            </el-form-item>\r\n                        </el-form>\r\n                        <div slot=\"footer\" class=\"dialog-footer\">\r\n                            <el-button type=\"primary\" @click=\"submitadd()\">添加</el-button>\r\n                            <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n                        </div>\r\n                    </el-dialog>\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!--      用户信息表格-->\r\n            <el-table\r\n                    :data=\"tableData\"\r\n                    border\r\n                    :row-style=\"{ height: '15px' }\"\r\n                    :cell-style=\"{ padding: '0px' }\"\r\n                    max-height=\"550\"\r\n                    style=\"width: 90%\"\r\n            >\r\n                <!--        <el-table-column fixed v-if=\"false\" prop=\"id\" label=\"用户id\" width=\"100\"></el-table-column>-->\r\n                <el-table-column fixed label=\"商品图片\" width=\"150\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-image v-if=\"scope.row.img !== ''\"\r\n                                  style=\"width: 80px; height: 80px\"\r\n                                  :src=\"scope.row.img\"\r\n                                  fit=\"contain\"\r\n                        ></el-image>\r\n                        <span v-else>无图片</span>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"name\" label=\"商品名称\" width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"price\" label=\"商品价格\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"point\" label=\"所需积分\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"amount\" label=\"库存数量\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"popularity\" label=\"已兑换数量\" width=\"120\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"kindName\" label=\"商品类型\" width=\"150\">\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" width=\"150\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button @click=\"handleClick(scope.row)\" type=\"primary\" size=\"small\">编辑</el-button>\r\n                        <el-button @click=\"deteleUser(scope.row)\" type=\"danger\" size=\"small\">删除</el-button>\r\n                        <!-- 编辑用户信息 -->\r\n                        <el-dialog\r\n                                title=\"修改商品信息\"\r\n                                :visible.sync=\"dialogFormVisible1\"\r\n                                width=\"40%\"\r\n                                append-to-body\r\n                                center\r\n                        >\r\n                            <el-upload\r\n                                    class=\"avatar-uploader\"\r\n                                    :action=\"doupload()\"\r\n                                    :show-file-list=\"false\"\r\n                                    :on-success=\"handleAvatarSuccess1\"\r\n                                    :before-upload=\"beforeAvatarUpload1\"\r\n                                    style=\"display: inline-block; width: 120px; margin-left: 35%;margin-bottom: 20px;\">\r\n                                <!-- 上传成功后展示的图片 -->\r\n                                <img v-if=\"form1.img\" :src=\"form1.img\" class=\"avatar\" />\r\n                                <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                            </el-upload>\r\n                            <el-form\r\n                                    :model=\"form1\"\r\n                                    style=\":form1\"\r\n                                    :rules=\"loginFormRules\"\r\n                                    ref=\"loginFormRef\"\r\n                            >\r\n                                <el-form-item\r\n                                        label=\"商品名称:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"name\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.name\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"商品价格:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"price\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.price\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"所需积分:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"point\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.point\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"库存数量:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"amount\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.amount\"\r\n                                            autocomplete=\"off\"\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item\r\n                                        label=\"已兑换数量:\"\r\n                                        :label-width=\"formLabelWidth\"\r\n                                        prop=\"popularity\"\r\n                                >\r\n                                    <el-input\r\n                                            v-model=\"form1.popularity\"\r\n                                            autocomplete=\"off\"\r\n                                            disabled\r\n                                    ></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"商品类型:\" :label-width=\"formLabelWidth\">\r\n                                    <el-select\r\n                                            v-model=\"form1.kindName\"\r\n                                            :placeholder=\"form1.kindName\"\r\n                                    >\r\n                                        <el-option\r\n                                                v-for=\"item in options\"\r\n                                                :key=\"item.value\"\r\n                                                :label=\"item.label\"\r\n                                                :value=\"item.value\"\r\n                                        >\r\n                                        </el-option>\r\n                                    </el-select>\r\n                                </el-form-item>\r\n                            </el-form>\r\n                            <div slot=\"footer\" class=\"dialog-footer\">\r\n                                <el-button type=\"primary\" @click=\"editdetail()\">提交</el-button>\r\n                                <el-button @click=\"dialogFormVisible1 = false\">取消</el-button>\r\n                            </div>\r\n                        </el-dialog>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <div class=\"block\">\r\n                <el-pagination\r\n                        background\r\n                        @size-change=\"handleSizeChange\"\r\n                        @current-change=\"handleCurrentChange\"\r\n                        :current-page.sync=\"currentPage\"\r\n                        :page-size=\"pagesize\"\r\n                        layout=\"prev, pager, next, jumper\"\r\n                        :total=\"total\"\r\n                >\r\n                </el-pagination>\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        created() {\r\n            this.userdata();\r\n        },\r\n\r\n        data() {\r\n            return {\r\n                options: [\r\n                    {\r\n                        value: \"虚拟商品\",\r\n                        label: \"虚拟商品\",\r\n                    },\r\n                    {\r\n                        value: \"普通商品\",\r\n                        label: \"普通商品\",\r\n                    },\r\n                    {\r\n                        value: \"高阶商品\",\r\n                        label: \"高阶商品\",\r\n                    },\r\n                ],\r\n                total: 1000,\r\n                // 分页\r\n                //currentPage: 1, //初始页\r\n                pagesize: 5, //每页多少数据\r\n                currentPage: 1, //默认当前页为第一页\r\n                //添加用户对话框\r\n                dialogFormVisible: false,\r\n                form: {\r\n                    img:\"https://cube.elemecdn.com/6/94/4d3ea53c084bad6931a56d5158a48jpeg.jpeg\",\r\n                    name: \"\",\r\n                    price: \"\",\r\n                    point: \"\",\r\n                    amount: \"\",\r\n                    popularity: \"\",\r\n                    kind:\"\",\r\n                },\r\n                //修改用户的对话框\r\n                dialogFormVisible1: false,\r\n                form1: {\r\n                    img:\"https://cube.elemecdn.com/6/94/4d3ea53c084bad6931a56d5158a48jpeg.jpeg\",\r\n                    name: \"\",\r\n                    price: \"\",\r\n                    point: \"\",\r\n                    amount: \"\",\r\n                    popularity: \"\",\r\n                    kind:\"\",\r\n                },\r\n                formLabelWidth: \"100px\",\r\n                formInline: {\r\n                    commodity: \"\",\r\n                },\r\n                tableData: [],\r\n                loginFormRules: {\r\n                    name: [\r\n                        { required: true, message: \"请正确输入商品名称\", trigger: \"blur\" },\r\n                    ],\r\n                    price: [\r\n                        { required: true, message: \"请正确输入商品价格\", trigger: \"blur\" },\r\n                    ],\r\n                    point: [\r\n                        { required: true, message: \"请正确输入所需积分\", trigger: \"blur\" },\r\n                    ],\r\n                    amount: [\r\n                        { required: true, message: \"请正确输入库存数量\", trigger: \"blur\" },\r\n                    ],\r\n                    kind: [\r\n                        { required: true, message: \"请选择商品种类\", trigger: \"blur\" },\r\n                    ],\r\n                },\r\n            };\r\n        },\r\n        methods: {\r\n            // 获取第一页表格数据\r\n            async userdata() {\r\n                const result = await this.$http.get(\r\n                    `/commodity/alllist?currentPage=${1}&size=${this.pagesize}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.tableData = result.data.data;\r\n                    this.total = Number(result.data.total);\r\n                    console.log(this.tableData);\r\n                }\r\n            },\r\n            handleClick(row) {\r\n                // 点击编辑获取信息\r\n                this.dialogFormVisible1 = true;\r\n                var tmp=Object.assign({},row);\r\n                this.form1 = tmp;\r\n                console.log(row);\r\n            },\r\n            handleSizeChange(val) {\r\n                console.log(`每页 ${val} 条`);\r\n            },\r\n            async handleCurrentChange(val) {\r\n                console.log(`当前页: ${val}`);\r\n                const result = await this.$http.get(\r\n                    `/commodity/alllist?currentPage=${val}&size=${5}`,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }\r\n                );\r\n                if (result.status === 200) {\r\n                    this.tableData = result.data.data;\r\n                    this.total = Number(result.data.total);\r\n                }\r\n            },\r\n            // 添加用户信息\r\n            submitadd() {\r\n                this.$refs.loginFormRef.validate(async (valid) => {\r\n                    if (!valid) return this.$message.error(\"表单验证失败！\");\r\n                    // 添加用户信息数据\r\n                    console.log(this.form);\r\n                    const result = await this.$http.post(\"/commodity/add\", this.form,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    });\r\n                    if (result.data.code === 200) {\r\n                        this.$message.success(\"添加成功!\");\r\n                        this.userdata();\r\n                        this.$forceUpdate();\r\n                        this.dialogFormVisible = false;\r\n                    } else if (result.data.code === 500) {\r\n                        this.$message.error(result.data.message);\r\n                    } else {\r\n                        this.$message.error(result.data.message);\r\n                    }\r\n                    //清除数据\r\n                    this.form = {\r\n                        img:\"https://cube.elemecdn.com/6/94/4d3ea53c084bad6931a56d5158a48jpeg.jpeg\",\r\n             