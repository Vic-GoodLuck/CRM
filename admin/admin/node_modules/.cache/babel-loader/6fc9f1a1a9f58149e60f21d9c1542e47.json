       <i class=\"el-icon-notebook-2\"></i>\r\n              <span>营销管理</span>\r\n            </template>\r\n            <el-menu-item-group>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>销售机会管理</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>客户开发计划</el-menu-item>\r\n            </el-menu-item-group>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"2\" v-show=\"show1\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-notebook-2\"></i>\r\n              <span>客户管理</span>\r\n            </template>\r\n            <el-menu-item-group>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>客户信息管理</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>客户流失管理</el-menu-item>\r\n            </el-menu-item-group>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"3\" v-show=\"show1\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-notebook-2\"></i>\r\n              <span>服务管理</span>\r\n            </template>\r\n            <el-menu-item-group>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>服务创建</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>服务分配</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>服务处理</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>服务反馈</el-menu-item>\r\n              <el-menu-item style=\"margin-left:50px;\" index=\"\"><i class=\"el-icon-tickets\"></i>服务归档</el-menu-item>\r\n            </el-menu-item-group>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"4\" v-show