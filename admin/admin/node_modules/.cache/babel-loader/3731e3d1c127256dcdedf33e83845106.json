\n            },\r\n            {\r\n                path:'/userdetail',\r\n                component:()=>import('@/views/user')\r\n            },\r\n            {\r\n                path:'/roledetail',\r\n                component:()=>import('@/views/role')\r\n            },\r\n            {\r\n                path:'/questiondetail',\r\n                component:()=>import('@/views/question')\r\n            },\r\n            {\r\n                path:'/commoditydetail',\r\n                component:()=>import('@/views/commodity')\r\n            },\r\n            {\r\n                path:'/orderdetail',\r\n                component:()=>import('@/views/order')\r\n            },\r\n            {\r\n                path:'/refunddetail',\r\n                component:()=>import('@/views/order/refund.vue')\r\n            },\r\n            // 视频播放管理\r\n            {\r\n                path:'/video',\r\n                component:()=>import('@/views/video')\r\n            },\r\n            {\r\n                path:'/space',\r\n                component:()=>import('@/views/space')\r\n            },\r\n            {\r\n                path:'/videodetail',\r\n                component:()=>import('@/views/video/play.vue')\r\n            },\r\n            {\r\n                path:'/videocheck',\r\n                component:()=>import('@/views/video/videocheck')\r\n            },\r\n            // 管理员页面\r\n           \r\n            {\r\n                path:'/edituser',\r\n                // 编辑个人资料\r\n                component:()=>import('@/components/editdetail.vue')\r\n            },\r\n            {\r\n                path:'/changepwd',\r\n                // 修改密码\r\n                component:()=>import('@/components/changepassword.vue')\r\n            },\r\n            {\r\n                path:'/404',\r\n                // 修改密码\r\n                component:()=>import('@/views/404.vue')\r\n            },\r\n\r\n        ]\r\n    }\r\n]\r\nconst routers = new VueRouter({routes})\r\nrouters.beforeEach(async (to, from, next) => {\r\n    // console.log('to',to)\r\n    // console.log('from',from)\r\n    // console.log('next',next)\r\n    var userinfo = JSON.parse(localStorage.getItem('userdata'))\r\n    console.log(userinfo)\r\n\r\n    if(userinfo){\r\n        if (to.path === '/login' || to.path === '/loginEmail') {\r\n            next('/home')\r\n        } else {\r\n            next()\r\n        }\r\n    }else{//未登录只能去登陆注册\r\n        if (to.path === '/login' || to.path === '/loginEmail') {\r\n            next()\r\n        } else {\r\n            next('/login')\r\n        }\r\n    }\r\n})\r\nexport default routers"]}]}