;AAGAC;AAHA,mBAKApB,IALA,uEAKA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCACA;AACAF;AACAC;AADA;AADA,6BAIAC,IAJA,CAIA;AACA;AACA;;AACA;AACA;AACA;;AACA;;AACA;AACA,+BALA,MAKA;AACA;AACA;AACA,6BAVA,MAUA;AACA;AACA;AACA,2BAlBA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBALA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BA,KA3CA;AA4CAqB,eA5CA,yBA4CA;AAAA;;AACA;AACA;AACAvB;AACAC;AADA;AADA,SAIAC,IAJA,CAIA;AACA;AACA;AACA;AACA;;AACA;AACA;AACA,OAXA;AAYA;AA1DA;AAzCA","names":["mounted","JSON","headers","Authorization","then","data","hasRole","pageName","currentPage","size","total","article","methods","jump","path","currentChange","sizeChange","detail","id","query","checkArticle","check","confirmButtonText","cancelButtonText","type","initArticle"],"sourceRoot":"src/views/article","sources":["articlecheck.vue"],"sourcesContent":["<!--文章列表-->\r\n<template>\r\n    <div>\r\n        <el-card class=\"box-card\" shadow=\"hover\" style=\"width: 100%\">\r\n            <el-breadcrumb style=\"margin-bottom:15px;\" separator=\"/\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item :to=\"{ path: '/articlelist' }\">文章管理</el-breadcrumb-item>\r\n                <el-breadcrumb-item>文章审核</el-breadcrumb-item>\r\n            </el-breadcrumb>\r\n            <el-divider></el-divider>\r\n            <div style=\"\r\n        display: flex;\r\n        margin-top: 30px;\r\n        margin-left: 10vh;\r\n        margin-right: 20vh;\r\n      \">\r\n                <div>\r\n                    <!-- 文章列表 -->\r\n                    <div v-for=\"(article, index) in article\" :key=\"index\">\r\n                        <el-card class=\"box-card\" shadow=\"hover\">\r\n                            <!-- 判断文章是否有图片，不同的布局方式 -->\r\n                            <div v-if=\"article.mainImg != ''\" style=\"display: flex\">\r\n                                <div class=\"article-img\" @click=\"detail(article.id)\"><img :src=\"article.mainImg\" /></div>\r\n                                <div @click=\"detail(article.id)\">\r\n                                    <!--                标题-->\r\n                                    <div class=\"article-title\" style=\"margin-left: 5vh; margin-top: 5vh\">\r\n                                        {{ article.title }}\r\n                                    </div>\r\n                                    <!--                简介-->\r\n                                    <div class=\"article-content1\">{{ article.mainContent }}</div>\r\n                                    <div style=\"display: flex\">\r\n                                        <!--                  时间-->\r\n                                        <div class=\"article-time\" style=\"margin-left: 5vh; margin-bottom: 2vh\">\r\n                                            <i class=\"el-icon-pie-chart\"></i>&nbsp;{{\r\n                                            article.gmtCreate\r\n                                            }}\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"article-button\">\r\n                                    <el-button class=\"article-button1\"\r\n                                            type=\"success\"\r\n                                            style=\"width:100px\"\r\n                                            @click=\"checkArticle(article.id,1)\">审核通过\r\n                                    </el-button>\r\n                                    <el-button class=\"article-button2\"\r\n                                            type=\"danger\"\r\n                                            style=\"width:100px\"\r\n                                            @click=\"checkArticle(article.id,-1)\">审核不通过\r\n                                    </el-button>\r\n                                </div>\r\n                            </div>\r\n                            <!-- 没有图片的布局-->\r\n                            <div v-else style=\"display: flex\">\r\n                                <div @click=\"detail(article.id)\">\r\n                                    <div class=\"article-title\" style=\"width: 750px\">\r\n                                        {{ article.title }}\r\n                                    </div>\r\n                                    <div class=\"article-content2\">{{ article.mainContent }}</div>\r\n                                    <div style=\"display: flex\">\r\n                                        <div class=\"article-time\">\r\n                                            <i class=\"el-icon-pie-chart\"></i>&nbsp;{{ article.gmtCreate}}\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"article-button\">\r\n                                    <el-button class=\"article-button1\"\r\n                                            type=\"success\"\r\n                                            style=\"width:100px\"\r\n                                            @click=\"checkArticle(article.id,1)\">审核通过\r\n                                    </el-button>\r\n                                    <el-button class=\"article-button2\"\r\n                                            type=\"danger\"\r\n                                            style=\"width:100px\"\r\n                                            @click=\"checkArticle(article.id,-1)\">审核不通过\r\n                                    </el-button>\r\n                                </div>\r\n                            </div>\r\n                        </el-card>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <el-pagination\r\n                    @current-change=\"currentChange\"\r\n                    @size-change=\"sizeChange\"\r\n                    :current-page.sync=\"currentPage\"\r\n                    :page-size=\"size\"\r\n                    layout=\"prev, pager, next, jumper, ->, total\"\r\n                    :total=\"total\"\r\n                    style=\"margin-left: 450px; margin-top: 20px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        mounted() {\r\n            this.$http.get(\r\n                `/role/getRole?uid=${JSON.parse(localStorage.getItem('userdata')).id}`,{\r\n                    headers: {\r\n                        Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                    }\r\n                }\r\n            ).then(res=>{\r\n                let RoleName=res.data.obj;\r\n                if(RoleName.includes(this.pageName)){\r\n                    this.hasRole=true;\r\n                }else{\r\n                    this.hasRole=false;\r\n                }\r\n                if(this.hasRole==false){\r\n                    this.$message.error(\"无权访问\");\r\n                    this.jump(\"/404\");\r\n                    return;\r\n                }\r\n                this.initArticle();\r\n            })\r\n        },\r\n        data() {\r\n            return {\r\n                hasRole:null,\r\n                pageName:\"文章审核\",\r\n\r\n                //分页\r\n                //初始页\r\n                currentPage: 1, //初始页\r\n                size: 4, //每页多少数据\r\n                total:0,\r\n                // 文章\r\n                article: [\r\n                    {\r\n\r\n                    }\r\n                ],\r\n            };\r\n        },\r\n        methods: {\r\n            //this.jump(\"/404\");\r\n            jump(path) {\r\n                this.$router.push(path);\r\n            },\r\n            currentChange(currentPage){\r\n                this.currentPage = currentPage;\r\n                this.initArticle();\r\n            },\r\n            sizeChange(size){\r\n                this.size = size;\r\n                this.initArticle();\r\n            },\r\n            detail(id) {\r\n                this.$router.push({ path: \"articledetail\", query: { id: id } });\r\n            },\r\n            async checkArticle(id,check) {\r\n                this.$confirm(\"请确认操作\", \"提示\", {\r\n                    confirmButtonText: \"确定\",\r\n                    cancelButtonText: \"取消\",\r\n                    type: \"warning\",\r\n                })\r\n                .then(async() => {\r\n                    await this.$http.put(\"/article/check?aid=\"+id+\"&check=\"+check,{\r\n                        headers: {\r\n                            Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                        }\r\n                    }).then((resp) => {\r\n                        if (resp) {\r\n                            let res = resp.data;\r\n                            if (res.code === 200) {\r\n                                // 审核成功\r\n                                this.$message.success(res.message);\r\n                                this.initArticle();\r\n                                this.$forceUpdate();\r\n                            } else {\r\n                                this.$message.error(res.message);\r\n                            }\r\n                        } else {\r\n                            this.$message.error(resp.data.message);\r\n                        }\r\n                    });\r\n                })\r\n            },\r\n            initArticle(){\r\n                // eslint-disable-next-line no-unused-vars\r\n                this.$http.get(\"/article/targetlist?currentPage=\"+this.currentPage+\"&size=\"+this.size+\"&checked=0\",{\r\n                    headers: {\r\n                        Authorization: \"Bearer \" + JSON.parse(localStorage.getItem('userdata')).token\r\n                    }\r\n                }).then(resp=>{\r\n                    // eslint-disable-next-line no-empty\r\n                    if(resp.status===200){\r\n                        this.total = resp.data.total;\r\n                        this.article = resp.data.data;\r\n                        this.$forceUpdate();\r\n                    }\r\n                })\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    // 文章列表\r\n    .box-card {\r\n        width: 960px;\r\n        text-align: left;\r\n        margin-top: 2vh;\r\n        border-radius: 15px;\r\n    }\r\n    // 文章文字css\r\n    .article-title {\r\n        display: inline-block;\r\n        white-space: nowrap;\r\n        width: 425px;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        color: #555555;\r\n        height: 30px;\r\n        line-height: 30px;\r\n        font-weight: bold;\r\n        font-size: 20px;\r\n    }\r\n    .article-img {\r\n        width: 400px;\r\n        height: 200px;\r\n        img {\r\n            width: 100%;\r\n            height: 100%;\r\n        }\r\n    }\r\n    .article-content1 {\r\n        width: 300px;\r\n        margin-left: 5vh;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        display: -webkit-box;\r\n        -webkit-box-orient: vertical;\r\n        -webkit-line-clamp: 3;\r\n        color: #888888;\r\n        font-size: 15px;\r\n    }\r\n    .article-content2 {\r\n        text-align: left;\r\n        width: 300px;\r\n        word-break: break-all;\r\n        display: -webkit-box;\r\n        -webkit-line-clamp: 3;\r\n        -webkit-box-orient: vertical;\r\n        overflow: hidden;\r\n        color: #888888;\r\n        font-size: 15px;\r\n    }\r\n    .article-button{\r\n        align-self: center;\r\n    }\r\n    .article-button1{\r\n        margin:10px;\r\n    }\r\n    .article-button2{\r\n        margin:10px;\r\n    }\r\n    .article-time {\r\n        color: #838383;\r\n        margin-top: 1vh;\r\n    }\r\n    .box-introduce {\r\n        width: 350px;\r\n        margin-left: 10vh;\r\n        border-radius: 15px;\r\n        margin-top: 2vh;\r\n    }\r\n    .link-color {\r\n        text-align: left;\r\n    }\r\n    .fontExtraLarge {\r\n        white-space: nowrap;\r\n        overflow: hidden !important;\r\n        text-overflow: ellipsis;\r\n        width: 250px;\r\n        margin-left: 20px;\r\n    }\r\n    .article-time {\r\n        color: #838383;\r\n        margin-top: 1vh;\r\n    }\r\n</style>\r\n"]}]}